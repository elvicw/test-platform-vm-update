sudo apt update
sudo apt upgrade -y
sudo apt full-upgrade -y
sudo apt autoremove -y

> update-deb

echo "sudo apt update' >> update-deb
echo "sudo apt upgrade -y" >> update-deb
echo "sudo apt full-upgrade -y" >> update-deb
echo "sudo apt autoremove -y" >> update-deb
echo "sudo reboot" >> update-deb
